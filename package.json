{
  "name": "inception-webapp",
  "version": "1.0.0",
  "description": "Inception Webapp",
  "author": "LuÃ­s (Quicksaver)",
  "license": "MIT",
  "scripts": {
    "build:development": "NODE_ENV=production env-cmd -f ./.env.development yarn next build",
    "build:production": "NODE_ENV=production env-cmd -f ./.env.production yarn next build",
    "dev": "npm-run-all --parallel dev:next dev:sanity dev:typegen",
    "dev:next": "yarn next dev",
    "dev:sanity": "sanity dev",
    "dev:typegen": "nodemon --watch ./studio/queries --watch ./studio/schema --ext ts --exec \"yarn sanity:schema-extract && yarn sanity:typegen\"",
    "lint:check": "yarn lint:stylelint && yarn lint:tsc && yarn lint:eslint && yarn lint:prettier",
    "lint:eslint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "yarn lint:stylelint --fix && yarn lint:tsc && yarn lint:eslint --fix && yarn lint:prettier --write",
    "lint:prettier": "prettier --log-level=warn --check .",
    "lint:stylelint": "stylelint **/*.scss",
    "lint:tsc": "tsc --noEmit",
    "sanity:schema-extract": "sanity schema extract --enforce-required-fields --path ./studio/sanity.schema.json",
    "sanity:typegen": "sanity typegen generate --config-path ./studio/typegen.json",
    "start": "NODE_ENV=production yarn next start"
  },
  "packageManager": "yarn@4.10.3",
  "browserslist": [
    ">0.3%, last 3 versions, Firefox ESR, not dead"
  ],
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.39.1",
    "@stylistic/eslint-plugin": "^5.5.0",
    "@types/confusing-browser-globals": "^1.0.3",
    "confusing-browser-globals": "^1.0.11",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.0.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-perfectionist": "^4.15.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "stylelint": "^16.25.0",
    "stylelint-color-format": "^1.1.0",
    "stylelint-config-clean-order": "^7.0.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard-scss": "^16.0.0",
    "stylelint-csstree-validator": "github:carlosjeurissen/stylelint-validator#stylelint-16",
    "stylelint-declaration-block-no-ignored-properties": "^2.8.0",
    "stylelint-order": "^7.0.0",
    "stylelint-scss": "^6.12.1",
    "stylelint-use-nesting": "^6.0.0",
    "typescript-eslint": "^8.46.3"
  },
  "dependencies": {
    "@portabletext/react": "^5.0.0",
    "@radix-ui/react-accessible-icon": "^1.1.8",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@sanity/icons": "^3.7.4",
    "@sanity/image-url": "^1.2.0",
    "@sanity/vision": "^4.14.1",
    "@types/google-libphonenumber": "^7.4.30",
    "@types/js-cookie": "^3.0.6",
    "@types/react-dom": "^19.2.2",
    "change-case": "^5.4.4",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "env-cmd": "^11.0.0",
    "events": "^3.3.0",
    "globals": "^16.5.0",
    "google-libphonenumber": "^3.2.43",
    "input-otp": "^1.4.2",
    "js-cookie": "^3.0.5",
    "next": "^16.0.1",
    "next-sanity": "^11.6.5",
    "npm-run-all": "^4.1.5",
    "posthog-js": "^1.288.1",
    "react": "^19.2.0",
    "react-day-picker": "^9.11.1",
    "react-dom": "^19.2.0",
    "react-international-phone": "^4.6.0",
    "react-toastify": "^11.0.5",
    "sanity": "^4.14.1",
    "sanity-plugin-media": "^4.0.0",
    "sass-embedded": "^1.93.3",
    "scss-reset": "^1.4.6",
    "styled-components": "^6.1.19",
    "typescript": "^5.9.3",
    "uuid": "^13.0.0",
    "zustand": "^5.0.8"
  }
}
